					onDropdownHide : function() {
						var guiData = {
							tableid : 'tbAppDetails',
							callback : FvApiCallbacks.getAppDetailsApi.updateAppDetailsTable
						};
						var evData = {
							guiData : guiData,
							getPostData : FvApiCallbacks.getAppDetailsApi.getPostData
						};
