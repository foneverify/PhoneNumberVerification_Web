    }

        .bannerknowmore a {
            display: block !important;
        }

    .bannerheader3 {
        padding: 0 15%;
    }

    .bannerheader4 {
        padding: 0 15%;
    }

    .bannerknowmore {
        padding: 5px 20px 0 20px !important;
    }

    .pos-relative {
        position: static;
    }

        .pos-relative .absolute-btm {
            position: static;
            right: 0px !important;
            bottom: -10px !important;
        }

            .pos-relative .absolute-btm img {
                margin: 0 auto !important;
            }

    .abtwhit-panel {
        min-height: initial;
    }

    .docsleft {
        width: 35% !important;
    }

    .docsright {
        width: 63% !important;
    }

    a.selector {
        height: auto !important;
        position: relative;
    }

        a.selector > label.credit, label.creditval {
            display: inline-block;
        }

        a.selector > label.creditval {
            float: right;
            width: 50% !important;
        }
}

