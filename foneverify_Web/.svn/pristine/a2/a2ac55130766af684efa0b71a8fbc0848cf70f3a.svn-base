@media screen and (min-width: 50px)and (max-width: 767px) {

    body {
        padding: 0px;
        margin: 0px !important;
        /*background: #f9f9f9!important;*/
    }

    .fleft, .fright {
        float: none;
        clear: both;
        display: block;
        width: 100%;
    }

    .padl0 {
        padding-left: 0px !important;
    }

    .deskdisplay {
        display: none !important;
    }

    .mobidisplay {
        display: block !important;
    }

    .gap-xl {
        height: 200px;
    }

    .gap-xl2 {
        height: 230px;
    }

    .banner-in .play-btn-text {
        display: block;
        margin: 0px 0 0 10px;
    }

    .banner-in {
        height: 100%;
        width: 100% !important;
    }

    .font40 {
        font-size: 30px !important;
    }

    .font30 {
        font-size: 25px !important;
    }

    .wrap-in {
        width: 90%;
        padding: 0px;
    }

        .wrap-in div:first-child {
            /* padding: 0px; */
        }

    .text-right {
        text-align: left !important;
    }

    .margin-auto {
        margin: 10px auto 0 auto !important;
    }

    .margin-0-auto {
        margin: 0px auto 0 auto !important;
    }

    .border-LR {
        border-width: 0px 0px 0px 0px;
        border-style: solid;
        border-color: #f1f1f1;
    }

    .border-TB {
        position: absolute;
        background: #ddd;
        width: 100%;
        height: 1px;
        left: 0px;
        top: 103% !important;
    }

        .border-TB:after, .border-TB:before {
            content: '';
            position: absolute !important;
            height: 100%;
            width: 0px !important;
        }

        .border-TB:before {
            left: 0px;
            top: 0px;
        }

        .border-TB:after {
            right: 0px !important;
            top: 0px;
        }


    .border-TBW {
        position: static;
        background: #ddd;
        width: 100%;
        height: 1px;
        box-shadow: #777 0 -1px 0;
        left: 0px;
        top: 103% !important;
        margin: 10px 0;
    }

        .border-TBW:after, .border-TBW:before {
            content: '';
            display: block;
            position: static !important;
            height: 2px;
            width: 100% !important;
        }

        .border-TBW:before {
            left: 0px;
            top: 0px;
        }

        .border-TBW:after {
            right: 0px !important;
            top: 0px;
        }



    .border-TB2 {
        margin: 30px 0 0px 0 !important;
        clear: both;
        display: block;
        padding: 0px !important;
        border-width: 2px 0px 0px 0px;
        border-style: solid;
        border-color: #ddd;
    }

        .border-TB2:before {
            content: '';
            display: block;
            clear: both;
            padding: 10px 0 !important;
        }

        .border-TB2:after {
            content: '';
            display: block;
            clear: both;
            padding: 10px 0;
        }


    .footer-nav li {
        display: block;
        padding:0px 0px!important
    }

        .footer-nav li a {
            display: block;
            text-align: left;
            padding: 5px 0;
        }

    .copyright {
        float: none;
        clear: both;
        display: block;
        margin: 0px 0;
        text-align: left;
        border-top: 1px dotted #aaa;
        padding: 5px 0;
    }

    .displaynone {
        display: none;
    }

    .fullwidth {
        width: 100% !important;
        display: block;
        padding: 10px 0;
    }


    .txtjustify {
    }

    .padd20, .padd30, .paddtb20, .paddtb30 {
        padding: 20px 0 0px 0 !important;
    }

    .paddtb20, .paddtb30 {
        padding: 0px 0 10px 0 !important;
    }

    .paddtop40 {
        padding-top: 0px !important;
    }

    .paddlr5, .paddlr10, .paddlr15, .paddlr20, .paddlr25, .paddlr30 {
