        border-radius: 10px;
        font-size: 30px;
        color: #6C265C;
        line-height: 40px;
    }


    .pagertabs > ul {
        border: 0px !important;
        margin: 0px !important;
        padding: 0px !important;
    }

        .pagertabs > ul li {
            display: block;
            float: none;
            clear: both;
            width: 100% !important;
            margin: 1px 0 !important;
            border-radius: 0px !important;
            border-bottom: 1px solid #bbb !important;
        }

            .pagertabs > ul li a {
                display: block !important;
                width: 100% !important;
                margin-bottom: -1px !important;
            }

    .dotarrowBG {
        background: none;
    }

    .paddtop30 {
        padding-top: 10px !important;
    }

    /*-------------------------------*/

    .ultabs {
        padding: 8px;
        border-bottom: 0px !important;
    }

        .ultabs > ul {
            padding: 0px;
        }

            .ultabs > ul li {
                float: none;
                /* display: block; */
				display:inline-block;
                clear: both;
                margin: 0px;
            }

                .ultabs > ul li a {
                    padding: 7px 20px;
                    /* max-width: 100% !important; */
                    display: block;
                    border-radius: 0px !important;
                    margin: 2px 0;
                    border-bottom: 1px solid #aaa;
                }

                .ultabs > ul li.current a {
                    color: #6C265C !important;
                    font-weight: bold;
                    background: #fff;
                    border-bottom: 1px solid #aaa;
                    margin-bottom: 1px;
                }

                .ultabs > ul li a:hover {
                    border-bottom: 1px solid #999;
                }


    nav {
        padding: 10px 0 0 0;
    }

    .navbar-collapse {
        overflow: auto !important;
    }

    .navbar-nav {
        margin-top: 0px !important;
        margin-bottom: 0px !important;
    }

        .navbar-nav li {
            padding: 0px !important;
            background: #f6f6f6;
        }

    .nav > li > a {
        margin: 0 0 1px 0px !important;
        display: block;
        padding: 8px 15px !important;
        position: relative;
    }


    .navbar-header {
        margin-top: -10px;
        padding: 8px 0px 8px 0px;
    }

    .navbar-nav {
    }

    li.editdropdown {
        border-bottom: 1px solid #eee !important;
        padding: 5px 14px 0 14px !important;
    }

        li.editdropdown:last-child {
            margin-bottom: -1px;
        }

    ul.navbarnav li.editdropdown > label {
        padding: 8px !important;
    }

    ul.navbarnav li.editdropdown > a .selector {
        padding: 10px !important;
    }

    li.editdropdown > ul {
        margin-top: 0px !important;
        border-top: 0px !important;
    }

    .collapse {
        border-bottom: 5px solid #f6f6f6 !important;
    }

    .navlabel, .usertextleft, .usertextright {
        padding: 1px 0 0 0 !important;
        margin: 0px;
    }

    .usertextleft, .usertextright {
        font-size: 14px;
    }

    .selector, .appsbox {
        border: 0px !important;
        text-align: left;
        padding: 5px;
        margin: 0px;
        background: #fff;
    }

    .dropdown-menu li {
        padding: 0px !important;
        margin: 0px !important;
    }

        .dropdown-menu li a {
            background: #f1f1f1;
        }

    .dropdown-menu:after, .dropdown-menu:before {
        display: none;
    }

    .navbar-toggle .icon-bar:nth-of-type(2) {
        top: 1px;
    }

    .navbar-toggle .icon-bar:nth-of-type(3) {
        top: 2px;
    }

    .navbar-toggle .icon-bar {
        position: relative;
        transition: linear 0.1s;
    }

    .navbar-toggle .icon-bar {
        height: 3px !important;
        display: block;
    }

    .navbar-toggle.active .icon-bar:nth-of-type(1) {
        top: 8px;
        transform: rotate(45deg);
    }

    .navbar-toggle.active .icon-bar:nth-of-type(2) {
        background-color: transparent !important;
    }

    .navbar-toggle.active .icon-bar:nth-of-type(3) {
        top: -6px;
        transform: rotate(-45deg);
    }

    .payment-table {
        width: 100%;
        padding: 10px !important;
    }

    .datext {
        display: block;
        padding: 10px 10px;
        border-bottom: 2px solid #ddd;
        background: #f9f9f9;
        margin-top: 20px;
    }

