.right-arrow:after {
	content: '';
    display: block;
    position: absolute;
    left: 100%;
    top: 51%;
    margin-top: -17px;
    width: 0;
    height: 0;
    border-top: 17px solid transparent;
    border-right: 10px solid transparent;
    border-bottom: 17px solid transparent;
    border-left: 30px solid rgb(108, 38, 92);
}



